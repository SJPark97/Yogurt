{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/react-in-jsx-scope": 0, // import React 생략 가능
    "react/jsx-props-no-spreading": "off",
    "react/prop-types": "off",
    "no-console": "off"
  }
}
